generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id      String  @id @default(uuid())
  title   String
  details String?
  slug    String  @unique
  maximumAtendees Int? @map("maximum_atendees") 

  @@map("events")
}